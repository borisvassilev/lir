#! /usr/bin/env bash
echo "Cleaning and uninstalling before re-building..."
make uninstall
make clean
echo "Done"
echo "Making and installing lire..."
make
echo "Done"
echo "Using lire to tangle and weave lire..."
lire lire.lir --bibliography=lire.bib
echo "Done"
echo "*** Weaved document is in lire.html ***"
